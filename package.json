    "axios": "^0.21.1",
    "axios-auth-refresh": "^3.2.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "eslint": "^8.10.0",
    "eslint-plugin-vue": "^9.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.5.1",
